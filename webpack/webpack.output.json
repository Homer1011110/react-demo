{
    "entry": {
        "a": "../src/a.js"
    },
    "output": {
        "filename": "[name].js",
        "chunkFilename": "[name].js",
        "path": "/Users/homerwang/projects/react-demo/static/js",
        "publicPath": "/js/"
    },
    "optimization": {
        "splitChunks": {
            "cacheGroups": {
                "vendors": {
                    "name": "vendors",
                    "chunks": "all",
                    "test": {}
                },
                "common": {
                    "name": "common",
                    "chunks": "all",
                    "test": {},
                    "minSize": 1,
                    "minChunks": 2,
                    "priority": 1
                }
            }
        }
    },
    "module": {
        "rules": [
            {
                "test": {},
                "exclude": {},
                "loader": "babel-loader",
                "options": {
                    "babelrcRoots": [
                        "../"
                    ]
                }
            }
        ]
    },
    "plugins": [
        {
            "options": {
                "context": null,
                "hashFunction": "md4",
                "hashDigest": "base64",
                "hashDigestLength": 4
            }
        },
        {
            "paths": [
                "/Users/homerwang/projects/react-demo/static"
            ],
            "options": {
                "allowExternal": true,
                "verbose": true,
                "dry": false,
                "root": "/Users/homerwang/projects/react-demo/webpack"
            }
        },
        {
            "options": {
                "template": "/Users/homerwang/projects/react-demo/src/tpls/index.tpl",
                "filename": "../index.html",
                "hash": false,
                "inject": true,
                "compile": true,
                "favicon": false,
                "minify": false,
                "cache": true,
                "showErrors": true,
                "chunks": "all",
                "excludeChunks": [],
                "chunksSortMode": "auto",
                "meta": {},
                "title": "Webpack App",
                "xhtml": false
            }
        }
    ],
    "resolve": {
        "extensions": [
            ".js",
            ".jsx"
        ]
    },
    "mode": "development",
    "devtool": "inline-source-map"
}