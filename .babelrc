{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": "> 0.25%, not dead"
            }
        ],
        [
            "@babel/preset-react",
            {
                "development": true
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", { "loose": false }],
        "@babel/plugin-syntax-dynamic-import",
    ]
}